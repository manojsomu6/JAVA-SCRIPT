//add using fuction
var a=6
var b=7
function add()
{
    console.log(a+b)
}
add()

// add using parameter
function add(a,b)
{
    console.log(a+b)
}
add(10,80)

//area formula
function area(l,b)
{
    var a=l*b
    console.log("Area is:"+a)
}
 var length=100
 var breadth=19
 area(100,19)

