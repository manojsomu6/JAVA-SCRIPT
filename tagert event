<h1 id="rs">name</h1>
<button onclick="update(event)">MANOJ</button>
<button  onclick="update(event)">JEEVA</button>
<button  onclick="update(event)">HARI</button>

<script>
    var h1=document.getElementById("rs")
    function update(event)
    {
        h1.textContent=event.target.textContent
    }

         
</script>
